{
  "name": "fuel-vite-plugin",
  "version": "1.0.5",
  "description": "Vite configuration helper for FuelPHP + Vue",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./inertia-helpers": {
      "types": "./dist/inertia-helpers/index.d.ts",
      "default": "./dist/inertia-helpers/index.js"
    }
  },
  "peerDependencies": {
    "@inertiajs/vue3": "^2.3.11",
    "vite": "^6.4.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "homepage": "https://github.com/Daiki52/fuel-vite-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/Daiki52/fuel-vite-plugin"
  },
  "keywords": [
    "fuelphp",
    "vite",
    "vue",
    "inertia"
  ],
  "license": "MIT"
}
